FoamFile
{
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101008;

boundaryField
{
    inlet
    {
        type            fixedValue;
	    value		    uniform 800000; // REVIEW: real value Rocket Propulsion Elements Book
    }

    outlet
    { // REVIEW: this value might not be fixed
        type            zeroGradient;
    }


    symmetry
    {
        type            symmetryPlane;
    }


    walls
    {
        type            zeroGradient;
    }


    front	
    {
        type            empty;
    }


    back
    {
        type            empty;
    }


    far_field
    {
        type            fixedValue;
	    value		    uniform 101008;
    }
}

