FoamFile
{
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101008; // 1 atm

boundaryField
{
    inlet
    {
        type            fixedValue;
	    value		    uniform 2e07; // 200 bar combustion chamber
    }

    outlet
    {
        type            zeroGradient;
    }


    symmetry
    {
        type            symmetryPlane;
    }


    walls
    {
        type            zeroGradient;
    }


    front	
    {
        type            empty;
    }


    back
    {
        type            empty;
    }


    far_field
    {
        type            fixedValue;
	    value		    uniform 101008; // 1 atm
    }
}

