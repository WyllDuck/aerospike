FoamFile
{
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Review: IMPORTANT
// Re = 10000: --> I = 0.16*Re**(-1/8) = 0.05
// k = (3/2)*(U_ref * I)**2 = 0.00375 


dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.00375;

boundaryField
{

    inlet
    {
        type            fixedValue;
        value           uniform 0.00375; // Review: IMPORTANT
    }

    outlet
    {
        type            zeroGradient;
    }

    symmetry
    {
        type            symmetryPlane;
    }

    walls
    {
        type            kqRWallFunction;
        value           uniform 0.00375;
    }

    front
    {
        type            empty;
    }

    back
    {
        type            empty;
    }

    far_field
    {
        type            fixedValue; // Review: IMPORTANT
        value           uniform 0.00375;
    }
}


