FoamFile
{
    format ascii;
    class volScalarField;
    object omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions[0 0 - 1 0 0 0 0];

internalField uniform ? ? ? ;

boundaryField
{

    inlet
    {
        type            fixedValue;
	    value		    uniform 2800;
    }

    outlet
    {
        type            fixedValue;
	    value		    uniform 298;
    }

    symmetry
    {
        type            symmetryPlane;
    }

    walls
    {
        type omegaWallFunction;
        Cmu 0.09;
        kappa 0.41;
        E 9.8;
        value uniform ? ? ? ? ;
    }
    }

    front
    {
        type            empty;
    }

    back
    {
        type            empty;
    }

    far_field
    {
        type turbulentMixingLengthFrequencyInlet;
        mixingLength 0.005;
        k k;
        value uniform ? ? ? ? ;
    }
}

// ************************************************************************* //
