FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue T 298
    volScalarFieldValue p 101008
);

regions
(
    boxToCell
    {
        box (-0.1 0 0) (0.1 0.18 0.09);
        fieldValues
        (
            volScalarFieldValue T 2800
            volScalarFieldValue p 2e07
        );
    }
);


